#include <iostream>

using namespace std;

void main()
{
	int year;
	cout << "Enter a year" << endl;
	cin >> year;

	
	if (year % 400 = 0 || year % 4 = 0)
	{
		cout << "not leap year" << endl;

	}

	if (year % 100 = 0 && year % 4 = 0)
	{
		cout << "leap year" << endl;
	}
	system("pause");
}
