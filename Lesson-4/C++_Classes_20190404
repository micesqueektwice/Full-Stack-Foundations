#include <iostream>

using namespace std;

class Rectangle

public:

double width;
double height;

Rectangle()
{
    width = 1.0;
    height = 1.0;
}

Rectangle(double measurewidth, double measureheight)
{
    //accepts two parameters that creates a rectangle with w X h = to value of parameters
}

void getArea() //returns area of rectangle
{
    getArea = width * height;
}

void getPerimeter() //returns perimeter of rectangle
{
    getPerimeter = 2(width + height);
}

void Rectangle::input() //object of Rectangle
{
    cin << "Height of rectangle" << width;
    cin << "Width of rectangle" << length;
}

void Rectangle::output() // object of Rectangle
{
    cout << "Area of rectangle is" << getArea 
    cout << "Perimeter of rectangle is" << getPerimeter << endl;
}

void main()
{
    Rectangle x;
    x.input();
    x.getArea();
    x.getPerimeter();
