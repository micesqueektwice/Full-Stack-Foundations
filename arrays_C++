	system("pause");


	void main()
	{//array = set of variables
		int x[5] = { 1, 4, 3, 1, 13 };
		int n = 113;
		bool found = false;
		for (int i = 0; i < 5; i++)
		{
			if (x[i] == n)
				cout << "Found at index " << i << endl;
				found = true;

		}
		if (!found)
			cout << "Not there" << endl;
	}
}

//recursive function

void main()
{//array = set of variables
	int x[5] = { 1, 4, 3, 1, 13 };
	for (int j = 0; j < 5; j++)

	{
		for (int i = 0; i < 4; i++)
		{ 
		if (x[i] > x[i + 1])
		{
			int temp = x[i];
			x[i] = x[i + 1];
			x[i + 1] = temp;
		}
	}
	
}
